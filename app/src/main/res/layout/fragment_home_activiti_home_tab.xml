<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActiviti$PlaceholderFragment">

    <ImageButton
        android:id="@+id/ibtn_scan"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginBottom="226dp"
        android:layout_marginEnd="227dp"
        android:layout_marginStart="228dp"
        android:layout_marginTop="459dp"
        android:onClick="onClick"
        android:src="@android:drawable/ic_menu_camera"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/ibtn_shop"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginBottom="85dp"
        android:layout_marginEnd="372dp"
        android:layout_marginStart="83dp"
        android:layout_marginTop="226dp"
        android:src="@android:drawable/checkbox_on_background"
        app:layout_constraintBottom_toTopOf="@+id/ibtn_scan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/ibtn_news"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginBottom="452dp"
        android:layout_marginEnd="82dp"
        android:layout_marginStart="145dp"
        android:layout_marginTop="226dp"
        android:src="@android:drawable/btn_star"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ibtn_shop"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>